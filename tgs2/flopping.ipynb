   "source": [
    "I = imread(‘cameraman.jpg’);\n",
    "[brs kol] = size(I);\n",
    "J = repmat(0,brs,kol);\n",
    "mirror = floor(kol/2);\n",
    "for x = 1 : brs-1\n",
    "for y = 1 : kol-1\n",
    "J(x,y) = I((2*mirror)-x, y);\n",
    "end\n",
    "end\n",
    "figure, imshow(uint8(J));"
   ]
  }
